<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.dsj.lims.dao.CheckitemMapper">
    <select id="getList" resultType="assessCheckitem">
        SELECT c.*,p.*,se.*,su.* FROM assess_checkitem AS c,`assess_partition` AS p,assess_selfevaluation AS se,assess_supervisor AS su
                                  WHERE c.checkId = p.partitionId AND c.checkId = se.`selfId` AND c.checkId= su.supervisorId
    </select>
    <insert id="addCheckitem">
        INSERT INTO `assess_checkitem`(`task1`,`task2`,`task3`,`task4`,`quality1`,`quality2`,`quality3`,`quality4`,`skill1`,`skill2`,`skill3`,`skill4`,`attitude1`,`attitude2`,`attitude3`,`attitude4`,`harmony1`,`harmony2`,`harmony3`,`harmony4`,`discipline1`,`discipline2`,`discipline3`,`discipline4`)
                              VALUES (#{task1},#{task2},#{task3},#{task4},#{quality1},#{quality2},#{quality3},#{quality4},#{skill1},#{skill2},#{skill3},#{skill4},#{attitude1},#{attitude2},#{attitude3},#{attitude4},#{harmony1},#{harmony2},#{harmony3},#{harmony4},#{discipline1},#{discipline2},#{discipline3},#{discipline4});
    </insert>
    <insert id="addPartition">
        INSERT INTO `assess_partition`(`partitionId`,`taskPartition1`, `taskPartition2`, `taskPartition3`, `taskPartition4`, `quaPartition1`, `quaPartition2`, `quaPartition3`, `quaPartition4`, `skillPartition1`, `skillPartition2`, `skillPartition3`, `skillPartition4`, `attPartition1`, `attPartition2`, `attPartition3`, `attPartition4`, `harPartition1`, `harPartition2`, `harPartition3`, `harPartition4`, `disPartition1`, `disPartition2`, `disPartition3`, `disPartition4`)
                              VALUES ( #{partitionId},#{taskPartition1} ,#{taskPartition2}, #{taskPartition3},#{taskPartition4}, #{quaPartition1},  #{quaPartition2},  #{quaPartition3},  #{quaPartition4}, #{skillPartition1}, #{skillPartition2}, #{skillPartition3},#{skillPartition4},#{attPartition1}, #{attPartition2}, #{attPartition3}, #{attPartition4}, #{harPartition1}, #{harPartition2},  #{harPartition3},  #{harPartition4}, #{disPartition1}, #{disPartition2},#{disPartition3}, #{disPartition4});
     </insert>
    <insert id="addSelfevaluation">
        INSERT INTO `assess_selfevaluation` ( `selfId`, `taskSelf1`, `taskSelf2`, `taskSelf3`, `taskSelf4`, `quaSelf1`, `quaSelf2`, `quaSelf3`, `quaSelf4`, `skillSelf1`, `skillSelf2`, `skillSelf3`, `skillSelf4`, `attSelf1`, `attSelf2`, `attSelf3`, `attSelf4`, `harSelf1`, `harSelf2`, `harSelf3`, `harSelf4`, `disSelf1`, `disSelf2`, `disSelf3`, `disSelf4`)
        VALUES ( #{selfId},#{taskSelf1} ,#{taskSelf2},#{taskSelf3},#{taskSelf4},#{quaSelf1},#{quaSelf2},#{quaSelf3},#{quaSelf4},#{skillSelf1},#{skillSelf2},#{skillSelf3},#{skillSelf4},#{attSelf1},#{attSelf2},#{attSelf3},#{attSelf4}, #{harSelf1}, #{harSelf2},#{harSelf3},#{harSelf4},#{disSelf1}, #{disSelf2}, #{disSelf3}, #{disSelf4});
    </insert>
    <insert id="addSupervisor">
        INSERT INTO `assess_supervisor` ( `supervisorId`, `taskSupervisor1`, `taskSupervisor2`, `taskSupervisor3`, `taskSupervisor4`, `quaSupervisor1`, `quaSupervisor2`, `quaSupervisor3`, `quaSupervisor4`, `skillSupervisor1`, `skillSupervisor2`, `skillSupervisor3`, `skillSupervisor4`, `attSupervisor1`, `attSupervisor2`, `attSupervisor3`, `attSupervisor4`, `harSupervisor1`, `harSupervisor2`, `harSupervisor3`, `harSupervisor4`, `disSupervisor1`, `disSupervisor2`, `disSupervisor3`, `disSupervisor4`)
                              VALUES (#{supervisorId}, #{taskSupervisor1},  #{taskSupervisor2}, #{taskSupervisor3}, #{taskSupervisor4}, #{quaSupervisor1},  #{quaSupervisor2}, #{quaSupervisor3}, #{quaSupervisor4},#{skillSupervisor1},#{skillSupervisor2},#{skillSupervisor3},#{skillSupervisor4}, #{attSupervisor1},#{attSupervisor2},#{attSupervisor3},#{attSupervisor4}, #{harSupervisor1}, #{harSupervisor2},#{harSupervisor3},#{harSupervisor4}, #{disSupervisor1}, #{disSupervisor2}, #{disSupervisor3}, #{disSupervisor4});

    </insert>
    <update id="updateCheckitem">
        UPDATE `assess_checkitem` SET `checkId` = #{checkId}, `task1` =#{task1}, `task2` =#{task2}, `task3` =#{task3}, `task4` = #{task4}, `quality1` = #{quality1}, `quality2` =#{quality2}, `quality3` =#{quality3}, `quality4` =#{quality4}, `skill1` =#{skill1}, `skill2` =#{skill2}, `skill3` =#{skill3}, `skill4` =#{skill4}, `attitude1` =#{attitude1}, `attitude2` =#{attitude2}, `attitude3` =#{attitude3}, `attitude4` =#{attitude4}, `harmony1` =#{harmony1}, `harmony2` =#{harmony2}, `harmony3` =#{harmony3}, `harmony4` =#{harmony4}, `discipline1` =#{discipline1}, `discipline2` =#{discipline2}, `discipline3` =#{discipline3}, `discipline4` =#{discipline4} WHERE checkId = #{checkId};
    </update>
    <update id="updatePartition">
        UPDATE assess_partition SET `id` = #{id}, `partitionId` =#{partitionId}, `taskPartition1` =#{taskPartition1}, `taskPartition2` =#{taskPartition2}, `taskPartition3` =#{taskPartition3}, `taskPartition4` =#{taskPartition4}, `quaPartition1` =#{quaPartition1}, `quaPartition2` =#{quaPartition2}, `quaPartition3` =#{quaPartition3}, `quaPartition4` =#{quaPartition4}, `skillPartition1` =#{skillPartition1}, `skillPartition2` =#{skillPartition2}, `skillPartition3` =#{skillPartition3}, `skillPartition4` =#{skillPartition4}, `attPartition1` =#{attPartition1}, `attPartition2` =#{attPartition2}, `attPartition3` =#{attPartition3}, `attPartition4` =#{attPartition4}, `harPartition1` =#{harPartition1}, `harPartition2` =#{harPartition2}, `harPartition3` = #{harPartition3}, `harPartition4` =#{harPartition4}, `disPartition1` =#{disPartition1}, `disPartition2` =#{disPartition2}, `disPartition3` =#{disPartition3}, `disPartition4` =#{disPartition4} WHERE `id` =#{id};
    </update>
    <update id="updateSelfevaluation">
        UPDATE assess_selfevaluation SET `id` =#{id}, `selfId` =#{selfId}, `taskSelf1` =#{taskSelf1}, `taskSelf2` = #{taskSelf2}, `taskSelf3` =#{taskSelf3}, `taskSelf4` =#{taskSelf4}, `quaSelf1` =#{quaSelf1}, `quaSelf2` =#{quaSelf2}, `quaSelf3` =#{quaSelf3}, `quaSelf4` =#{quaSelf4}, `skillSelf1` =#{skillSelf1}, `skillSelf2` =#{skillSelf2}, `skillSelf3` =#{skillSelf3}, `skillSelf4` =#{skillSelf4}, `attSelf1` =#{attSelf1}, `attSelf2` =#{attSelf2}, `attSelf3` =#{attSelf3}, `attSelf4` =#{attSelf4}, `harSelf1` =#{harSelf1}, `harSelf2` =#{harSelf2}, `harSelf3` = #{harSelf3}, `harSelf4` =#{harSelf4}, `disSelf1` =#{disSelf1}, `disSelf2` =#{disSelf2}, `disSelf3` =#{disSelf3}, `disSelf4` =#{disSelf4} WHERE `id` =#{id};
    </update>
    <update id="updateSupervisor">
        UPDATE assess_supervisor SET `id` =#{id}, `supervisorId` =#{supervisorId}, `taskSupervisor1` =#{taskSupervisor1}, `taskSupervisor2` =#{taskSupervisor2}, `taskSupervisor3` =#{taskSupervisor3}, `taskSupervisor4` =#{taskSupervisor4}, `quaSupervisor1` =#{quaSupervisor1}, `quaSupervisor2` =#{quaSupervisor2}, `quaSupervisor3` =#{quaSupervisor3}, `quaSupervisor4` =#{quaSupervisor4}, `skillSupervisor1` =#{skillSupervisor1}, `skillSupervisor2` =#{skillSupervisor2}, `skillSupervisor3` = #{skillSupervisor3}, `skillSupervisor4` =#{skillSupervisor4}, `attSupervisor1` =#{attSupervisor1}, `attSupervisor2` =#{attSupervisor2}, `attSupervisor3` =#{attSupervisor3}, `attSupervisor4` =#{attSupervisor4}, `harSupervisor1` =#{harSupervisor1}, `harSupervisor2` =#{harSupervisor2}, `harSupervisor3` =#{harSupervisor3}, `harSupervisor4` =#{harSupervisor4}, `disSupervisor1` =#{disSupervisor1}, `disSupervisor2` =#{disSupervisor2}, `disSupervisor3` =#{disSupervisor3}, `disSupervisor4` =#{disSupervisor4} WHERE `id` =#{id};
    </update>
    <delete id="deleteCheckitem">
        DELETE FROM `assess_checkitem` WHERE `checkId` = #{checkId};
    </delete>
    <delete id="deletePartition">
        DELETE FROM assess_partition WHERE id =#{id};
    </delete>
    <delete id="deleteSelfevaluation">
        DELETE FROM `assess_selfevaluation` WHERE `id` =#{id};
    </delete>
    <delete id="deleteSupervisor">
        DELETE FROM `assess_supervisor` WHERE `id` =#{id};
    </delete>

</mapper>